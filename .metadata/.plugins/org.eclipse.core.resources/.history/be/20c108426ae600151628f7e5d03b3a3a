package com.dearbear.classStudy;
public class CClassInfoStudy extends Shap implements ChangeShp{
	final double PI = 3.141592653;
	InClass m_inclass = new InClass();		
	public static void main(String args[]) {
		
		System.out.println("hello");
		System.out.printf("%d", );
	}
	
	@Override 
	public void draw(){
		System.out.println("override draw now");
	}
	
	@Override 
	public void change(){
		System.out.println("override change now");
	}
	private class InClass{
		public int m_x = 0, m_y = 0;
		private void test() {
			double length = PI * 2;
		}
	}
}

abstract class Shap{
	int m_x, m_y;
	abstract void draw();
}

interface ChangeShp{
	void change();
}
